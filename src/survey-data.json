{
  "summary": true,
  "showPageNumber": true,
  "showProgressBar": true,
  "showBackButton": true,
  "showCloseButton": true,
  "title": "Yieldstreet Survey",
  "colors": {
    "primary": "#4285f4",
    "secondary": "#fff",
    "fontColorPrimary": "#fff",
    "fontColorSecondary": "#000"
  },
  "pages": [
    {
      "title": "Identity",
      "inputs": [
        {
          "name": "name",
          "type": "text"
        },
        {
          "name": "email",
          "type": "email"
        }
      ]
    },
    {
      "title": "Details",
      "inputs": [
        {
          "name": "age",
          "type": "range",
          "range": [0, 150],
          "required": true
        },
        {
          "name": "gender",
          "type": "radio",
          "options": ["Male", "Female"],
          "required": true
        }
      ]
    },
    {
      "title": "Favorites",
      "inputs": [
        {
          "name": "favorite_book",
          "label": "Favorite Book",
          "type": "text",
          "required": true
        },
        {
          "name": "favorite_colors",
          "label": "Favorite Colors",
          "type": "checkbox",
          "multiple": true,
          "required": true,
          "options": [
            "red", "orange", "yellow", "green", "blue",
            "purple", "pink", "white", "gray", "brown",
            "black"
          ]
        }
      ]
    }
  ]
}